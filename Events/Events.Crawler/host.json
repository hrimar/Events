{
  "version": "2.0",
  "functionTimeout": "00:10:00", // "00:50:00",
  "extensions": {
    "http": {
      "maxConcurrentRequests": 100, // 200
      "maxOutstandingRequests": 200, // 400
      "routePrefix": "api"
    }
  },
  "logging": {
    "logLevel": {
      "default": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}